check process ss_local with MATCHING '.*ss-local.*config_local.json'
    start program = "/tmp/mnt/ASUS_ROUTER/script_bootloader/usr/shadowsocks/bin/ss_local_enable.service"
    stop program = "/tmp/mnt/ASUS_ROUTER/script_bootloader/usr/shadowsocks/bin/ss_local_disable.service"
    if does not exist then restart
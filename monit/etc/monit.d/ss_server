check process ss_server with MATCHING '.*ss-server.*config_server.json'
    start program = "/tmp/mnt/ASUS_ROUTER/script_bootloader/usr/shadowsocks/bin/ss_server_enable.service"
    stop program = "/tmp/mnt/ASUS_ROUTER/script_bootloader/usr/shadowsocks/bin/ss_server_disable.service"
    if does not exist then restart
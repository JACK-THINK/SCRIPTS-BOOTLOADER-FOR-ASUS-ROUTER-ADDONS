check process v2ray_client with MATCHING '.*v2ray.*config_client.json'
    start program = "/tmp/mnt/ASUS_ROUTER/script_bootloader/usr/v2ray/bin/v2ray_client_enable.service"
    stop program = "/tmp/mnt/ASUS_ROUTER/script_bootloader/usr/v2ray/bin/v2ray_client_disable.service"
    if does not exist then restart
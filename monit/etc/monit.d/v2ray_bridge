check process v2ray_bridge with MATCHING '.*v2ray.*config_bridge.json'
    start program = "/tmp/mnt/ASUS_ROUTER/script_bootloader/usr/v2ray/bin/v2ray_bridge_enable.service"
    stop program = "/tmp/mnt/ASUS_ROUTER/script_bootloader/usr/v2ray/bin/v2ray_bridge_disable.service"
    if does not exist then restart
check process v2ray_server with MATCHING '.*v2ray.*config_server.json'
    start program = "/tmp/mnt/ASUS_ROUTER/script_bootloader/usr/v2ray/bin/v2ray_server_enable.service"
    stop program = "/tmp/mnt/ASUS_ROUTER/script_bootloader/usr/v2ray/bin/v2ray_server_disable.service"
    if does not exist then restart
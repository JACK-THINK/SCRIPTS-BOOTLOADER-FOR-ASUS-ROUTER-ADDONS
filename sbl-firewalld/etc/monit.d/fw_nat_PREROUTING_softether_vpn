check program FW_PORT_FORWARDING_SOFTETHER_VPN with path "/opt/script_bootloader/usr/sbl-firewalld/bin/fw_nat_PREROUTING_softether_vpn_enable.service" every 4 cycles
    start program = "/opt/script_bootloader/usr/sbl-firewalld/bin/fw_nat_PREROUTING_softether_vpn_enable.service"
    stop program = "/opt/script_bootloader/usr/sbl-firewalld/bin/fw_nat_PREROUTING_softether_vpn_disable.service"
    if status == 1 then alert
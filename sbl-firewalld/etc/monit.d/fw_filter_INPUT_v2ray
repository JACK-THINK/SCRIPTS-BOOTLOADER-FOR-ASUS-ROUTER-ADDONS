    start program = "/opt/script_bootloader/usr/sbl-firewalld/bin/fw_filter_INPUT_v2ray_enable.service"
    stop program = "/opt/script_bootloader/usr/sbl-firewalld/bin/fw_filter_INPUT_v2ray_disable.service"

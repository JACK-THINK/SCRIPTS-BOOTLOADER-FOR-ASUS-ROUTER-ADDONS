check program FW_PORT_OPENING_V2RAY_PROXY with path "/opt/script_bootloader/usr/sbl-firewalld/bin/fw_filter_INPUT_v2ray_proxy_enable.service" every 6 cycles
    start program = "/opt/script_bootloader/usr/sbl-firewalld/bin/fw_filter_INPUT_v2ray_proxy_enable.service"
    stop program = "/opt/script_bootloader/usr/sbl-firewalld/bin/fw_filter_INPUT_v2ray_proxy_disable.service"
    if status == 1 then alert
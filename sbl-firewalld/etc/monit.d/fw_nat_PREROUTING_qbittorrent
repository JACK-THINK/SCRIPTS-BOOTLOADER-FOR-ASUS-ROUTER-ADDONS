check program FW_PORT_FORWARDING_QBITTORRENT with path "/opt/script_bootloader/usr/sbl-firewalld/bin/fw_nat_PREROUTING_qbittorrent_enable.service" every 4 cycles
    start program = "/opt/script_bootloader/usr/sbl-firewalld/bin/fw_nat_PREROUTING_qbittorrent_enable.service"
    stop program = "/opt/script_bootloader/usr/sbl-firewalld/bin/fw_nat_PREROUTING_qbittorrent_disable.service"
    if status == 1 then alert
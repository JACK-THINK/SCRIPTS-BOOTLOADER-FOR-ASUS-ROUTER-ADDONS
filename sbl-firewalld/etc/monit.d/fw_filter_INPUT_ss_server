    start program = "/opt/script_bootloader/usr/sbl-firewalld/bin/fw_filter_INPUT_ss_server_enable.service"
    stop program = "/opt/script_bootloader/usr/sbl-firewalld/bin/fw_filter_INPUT_ss_server_disable.service"
    if status == 1 then alert
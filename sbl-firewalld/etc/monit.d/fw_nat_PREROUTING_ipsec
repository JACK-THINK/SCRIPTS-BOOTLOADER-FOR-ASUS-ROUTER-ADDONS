check program FW_PORT_FORWARDING_IPSEC with path "/opt/script_bootloader/usr/sbl-firewalld/bin/fw_nat_PREROUTING_ipsec_enable.service" every 6 cycles
    start program = "/opt/script_bootloader/usr/sbl-firewalld/bin/fw_nat_PREROUTING_ipsec_enable.service"
    stop program = "/opt/script_bootloader/usr/sbl-firewalld/bin/fw_nat_PREROUTING_ipsec_disable.service"
    if status == 1 then alert